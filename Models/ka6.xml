  <!--Canopy handle LEFT-->
  
    <animation>
        <type>slider</type>
        <object-name>canopyhandle.left1</object-name>
        <object-name>canopyhandle.left2</object-name>
        <drag-direction>horizontal</drag-direction>
        <action>
            <binding>
                <condition>
                    <equals>
                    <property>sim/model/door-positions/canopy/position-norm</property>
                    <value>0</value>
                </equals>
                </condition>
                <command>property-adjust</command>
                <property>controls/canopy/lock-left</property>
                <factor>-0.1</factor>
                <min>0</min>
                <max>1</max>
                <wrap>0</wrap>
            </binding>
        </action>
        <hovered>
            <binding>
                <command>set-tooltip</command>
                <tooltip-id>canopyL</tooltip-id>
                <label>Canopy Lock Left: %2.0f</label>
                <property>controls/canopy/lock-left</property>
            </binding>
        </hovered>
    </animation>
    
    <animation>
        <type>translate</type>
        <object-name>canopyhandle.left1</object-name>
        <object-name>canopyhandle.left2</object-name>
                <property>controls/canopy/lock-left</property>
        <factor>0.05</factor>
        <axis>
            <x1-m>1.02097</x1-m>
            <y1-m>-0.2578</y1-m>
            <z1-m>0.29843</z1-m>
            <x2-m>1.98815</x2-m>
            <y2-m>-0.2703</y2-m>
            <z2-m>0.47223</z2-m>
        </axis>
    </animation>
