<?xml version="1.0"?>
<PropertyList>
    <path>interior.ac</path>
    <!--COCKPIT-->
    <!-- Yaw string -->
    <model>
        <name>yawstring</name>
        <path>Instruments/wool-thread/wool-thread.xml</path>
        <offsets>
            <x-m>-3.50</x-m>
            <y-m> 0</y-m>
            <z-m>0.90</z-m>
            <pitch-deg>0</pitch-deg>
        </offsets>
    </model><!--
    <model>
        <name>panelF</name>
        <path>Aircraft/ASK21/Models/Interior/Panel/panelF.xml</path>
    </model>
    <model>
        <name>panelR</name>
        <path>Aircraft/ASK21/Models/Interior/Panel/panelR.xml</path>
    </model>-->
    <!--Pilots--><!--
    <model>
        <name>pilots</name>
        <path>Aircraft/ASK21/Models/Interior/Pilots/pilots.xml</path>
    </model>-->
    <!--Small windows--><!--
    <animation>
        <type>slider</type>
        <object-name>sliding_window.front</object-name>
        <visible>true</visible>
        <action>
            <button>0</button>
            <repeatable>false</repeatable>
            <binding>
                <command>property-adjust</command>
                <property>sim/model/door-positions/frontwin/position-norm</property>
                <factor>-0.05</factor>
                <min>0</min>
                <max>1</max>
            </binding>
        </action>
    </animation>
    <animation>
        <type>translate</type>
        <object-name>sliding_window.front</object-name>
        <property>sim/model/door-positions/frontwin/position-norm</property>
        <interpolation>
            <entry>
                <ind>0</ind>
                <dep>0</dep>
            </entry>
            <entry>
                <ind>0.1</ind>
                <dep>0.01</dep>
            </entry>
            <entry>
                <ind>1</ind>
                <dep>0.01</dep>
            </entry>
        </interpolation>
        <axis>
            <y>1</y>
        </axis>
    </animation>
    <animation>
        <type>translate</type>
        <object-name>sliding_window.front</object-name>
        <property>sim/model/door-positions/frontwin/position-norm</property>
        <interpolation>
            <entry>
                <ind>0</ind>
                <dep>0</dep>
            </entry>
            <entry>
                <ind>0.1</ind>
                <dep>0.0</dep>
            </entry>
            <entry>
                <ind>1</ind>
                <dep>0.18</dep>
            </entry>
        </interpolation>
        <axis>
            <x1-m>-2.94858</x1-m>
            <y1-m>-0.31449</y1-m>
            <z1-m>0.70963</z1-m>
            <x2-m>-2.63997</x2-m>
            <y2-m>-0.32957</y2-m>
            <z2-m>0.76366</z2-m>
        </axis>
    </animation>
    <animation>
        <type>slider</type>
        <object-name>sliding_window.back</object-name>
        <visible>true</visible>
        <action>
            <button>0</button>
            <repeatable>false</repeatable>
            <binding>
                <command>property-adjust</command>
                <property>sim/model/door-positions/rearwin/position-norm</property>
                <factor>0.05</factor>
                <min>0</min>
                <max>1</max>
            </binding>
        </action>
    </animation>
    <animation>
        <type>translate</type>
        <object-name>sliding_window.back</object-name>
        <property>sim/model/door-positions/rearwin/position-norm</property>
        <interpolation>
            <entry>
                <ind>0</ind>
                <dep>0</dep>
            </entry>
            <entry>
                <ind>0.1</ind>
                <dep>0.01</dep>
            </entry>
            <entry>
                <ind>1</ind>
                <dep>0.01</dep>
            </entry>
        </interpolation>
        <axis>
            <y>1</y>
        </axis>
    </animation>
    <animation>
        <type>translate</type>
        <object-name>sliding_window.back</object-name>
        <property>sim/model/door-positions/rearwin/position-norm</property>
        <interpolation>
            <entry>
                <ind>0</ind>
                <dep>0</dep>
            </entry>
            <entry>
                <ind>0.1</ind>
                <dep>0.0</dep>
            </entry>
            <entry>
                <ind>1</ind>
                <dep>0.12</dep>
            </entry>
        </interpolation>
        <axis>
            <x1-m>-1.80509</x1-m>
            <y1-m>-0.31714</y1-m>
            <z1-m>0.92524</z1-m>
            <x2-m>-2.0615</x2-m>
            <y2-m>-0.31714</y2-m>
            <z2-m>0.90689</z2-m>
        </axis>
    </animation>-->
    <!--airbrake grip--><!--
    <animation>
        <type>slider</type>
        <object-name>grip.airbrake</object-name>
        <visible>true</visible>
        <action>
            <button>0</button>
            <repeatable>false</repeatable>
            <binding>
                <command>property-adjust</command>
                <property>/controls/engines/engine/throttle</property>
                <factor>0.05</factor>
                <min>0</min>
                <max>1</max>
            </binding>
        </action>
    </animation>
    <animation>
        <type>translate</type>
        <object-name>grip.airbrake</object-name>
        <property>/controls/engines/engine/throttle</property>
        <factor>1</factor>
        <interpolation>
            <entry>
                <ind>1</ind>
                <dep>0</dep>
            </entry>
            <entry>
                <ind>0</ind>
                <dep>.25</dep>
            </entry>
        </interpolation>
        <axis>
            <x>1</x>
            <y>0</y>
            <z>0</z>
        </axis>
    </animation>
    <animation>
        <type>translate</type>
        <object-name>grip.airbrake</object-name>
        <property>/controls/engines/engine/throttle</property>
        <factor>-1</factor>
        <interpolation>
            <entry>
                <ind>1</ind>
                <dep>0</dep>
            </entry>
            <entry>
                <ind>0</ind>
                <dep>-0.025</dep>
            </entry>
        </interpolation>
        <axis>
            <x>0</x>
            <y>1</y>
            <z>0</z>
        </axis>
    </animation>
    <animation>
        <type>translate</type>
        <object-name>grip.airbrake</object-name>
        <property>/controls/engines/engine/throttle</property>
        <factor>1</factor>
        <interpolation>
            <entry>
                <ind>1</ind>
                <dep>0</dep>
            </entry>
            <entry>
                <ind>0</ind>
                <dep>.045</dep>
            </entry>
        </interpolation>
        <axis>
            <x>0</x>
            <y>0</y>
            <z>1</z>
        </axis>
    </animation>-->
